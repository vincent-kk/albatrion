{
  "version": "0.0.0",
  "description": "none",
  "license": "MIT",
  "scripts": {
    "schemaForm": "yarn workspace @canard/schema-form",
    "antdPlugin": "yarn workspace @canard/schema-form-antd-plugin",
    "antdMobilePlugin": "yarn workspace @canard/schema-form-antd-mobile-plugin",
    "promiseModal": "yarn workspace @lerx/promise-modal",
    "benchmark": "yarn workspace @aileron/benchmark",
    "commonUtils": "yarn workspace @winglet/common-utils",
    "reactUtils": "yarn workspace @winglet/react-utils",
    "developmentHelper": "yarn workspace @aileron/development-helper",
    "run:all": "yarn workspaces foreach --all --topological run",
    "patch:all": "yarn workspaces foreach --all --topological --no-private run version:patch",
    "minor:all": "yarn workspaces foreach --all --topological --no-private run version:minor",
    "major:all": "yarn workspaces foreach --all --topological --no-private run version:major",
    "publish:all": "yarn workspaces foreach --all --topological --no-private run publish:npm",
    "login:npm": "yarn npm login --publish"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "packageManager": "yarn@4.8.0"
}
