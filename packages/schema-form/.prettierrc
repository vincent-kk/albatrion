{
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "<THIRD_PARTY_MODULES>",
    "^(\\@lumy-pack).*",
    "^(\\@lumy-pack)/(.*)$",
    "^(\\@lumy-form).*",
    "^(\\@lumy-form)/(.*)$",
    "^([../])",
    "^([./])"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}