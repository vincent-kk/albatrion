{
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^(?!([../]|[./]|[@lumy-pack])).*",
    "^@lumy-pack/(.*)$",
    "^([../])",
    "^([./])"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}