{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM"
    ],
    "baseUrl": "../../",
    "outDir": "./dist",
    "paths": {
      "@/data-loader": [
        "./winglet/data-loader/src"
      ],
      "@/data-loader/*": [
        "./winglet/data-loader/src/*"
      ],
      "@aileron/*": [
        "./aileron/common/*"
      ]
    }
  },
  "include": [
    "src",
    "src/**/*.tsx?"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}