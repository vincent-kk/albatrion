{
  "name": "@canard/schema-form-antd-mobile-plugin",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vincent-kk/albatrion.git",
    "directory": "packages/canard/schema-form-antd-mobile-plugin"
  },
  "license": "MIT",
  "author": {
    "name": "Vincent K. Kelvin",
    "email": "lunox273@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "yarn clean && rollup -c",
    "build-storybook": "storybook build",
    "build:chain": "yarn clean && yarn workspaces foreach --recursive --topological run build",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "publish:chain": "yarn build:chain && yarn npm publish --access public",
    "publish:npm": "yarn build && yarn npm publish --access public",
    "start": "yarn build && yarn storybook",
    "storybook": "storybook dev -p 6006",
    "storybook:upgrade": "npx storybook@latest upgrade",
    "test": "yarn build:chain && vitest",
    "version:major": "yarn version major",
    "version:minor": "yarn version minor",
    "version:patch": "yarn version patch"
  },
  "dependencies": {
    "@winglet/common-utils": "workspace:^",
    "@winglet/react-utils": "workspace:^",
    "antd-mobile": "^5.0.0",
    "dayjs": "^1.0.0"
  },
  "devDependencies": {
    "@canard/schema-form": "workspace:^",
    "@chromatic-com/storybook": "^3.2.6",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/rollup-plugin-peer-deps-external": "^2.2.5",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^0.12.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.27.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "storybook": "^8.6.14",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vitest": "^2.1.5"
  },
  "peerDependencies": {
    "dayjs": ">=1",
    "react": ">=18 <20",
    "react-dom": ">=18 <20"
  }
}
